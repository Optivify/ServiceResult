<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net6.0;net7.0;net8.0</TargetFrameworks>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<Company>Optivify</Company>
		<RepositoryUrl>https://github.com/Optivify/ServiceResult</RepositoryUrl>
		<AssemblyVersion></AssemblyVersion>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Description>A result object contains value and status of an operation which can be returned from services.</Description>
		<PackageProjectUrl>https://github.com/Optivify/ServiceResult</PackageProjectUrl>
		<Authors>Optivify</Authors>
		<Version>8.0.0</Version>
		<PackageTags>optivify service result pattern</PackageTags>
	</PropertyGroup>

</Project>
